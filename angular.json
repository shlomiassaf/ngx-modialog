{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-modialog": {
      "root": "projects/ngx-modialog",
      "sourceRoot": "projects/ngx-modialog/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-modialog/tsconfig.lib.json",
            "project": "projects/ngx-modialog/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ngx-modialog/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-modialog/src/test.ts",
            "tsConfig": "projects/ngx-modialog/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-modialog/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-modialog/tsconfig.lib.json",
              "projects/ngx-modialog/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "root": "projects/demo/",
      "sourceRoot": "projects/demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "distLib": {
              "tsConfig": "projects/demo/tsconfig.dist-lib.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "distLib": {
              "browserTarget": "demo:build:distLib"
            },
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.spec.json",
            "karmaConfig": "projects/demo/karma.conf.js",
            "styles": [
              "projects/demo/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo/tsconfig.app.json",
              "projects/demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo-e2e": {
      "root": "projects/demo-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo-e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/demo-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "bootstrap": {
      "root": "projects/plugins/bootstrap",
      "sourceRoot": "projects/plugins/bootstrap/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/plugins/bootstrap/tsconfig.lib.json",
            "project": "projects/plugins/bootstrap/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/plugins/bootstrap/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/plugins/bootstrap/src/test.ts",
            "tsConfig": "projects/plugins/bootstrap/tsconfig.spec.json",
            "karmaConfig": "projects/plugins/bootstrap/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/plugins/bootstrap/tsconfig.lib.json",
              "projects/plugins/bootstrap/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "vex": {
      "root": "projects/plugins/vex",
      "sourceRoot": "projects/plugins/vex/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/plugins/vex/tsconfig.lib.json",
            "project": "projects/plugins/vex/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/plugins/vex/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/plugins/vex/src/test.ts",
            "tsConfig": "projects/plugins/vex/tsconfig.spec.json",
            "karmaConfig": "projects/plugins/vex/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/plugins/vex/tsconfig.lib.json",
              "projects/plugins/vex/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "js-native": {
      "root": "projects/plugins/js-native",
      "sourceRoot": "projects/plugins/js-native/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/plugins/js-native/tsconfig.lib.json",
            "project": "projects/plugins/js-native/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/plugins/js-native/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/plugins/js-native/src/test.ts",
            "tsConfig": "projects/plugins/js-native/tsconfig.spec.json",
            "karmaConfig": "projects/plugins/js-native/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/plugins/js-native/tsconfig.lib.json",
              "projects/plugins/js-native/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo",
  "cli": {
    "packageManager": "yarn"
  }
}
